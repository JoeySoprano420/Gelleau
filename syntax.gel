sum(a, b) -> (a + b);

# Conditional logic
if x > 10; {
    print("High");
} else {
    print("Low");
}

# Looping (register-optimized iteration)
while r0 < 100; {
    r0 += 5;
}

# Memory persistence directives
memflag { temp:x, perm:y };

# Hook inline assembly
asm("mov r0, r1");

